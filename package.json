{
  "name": "flipper-ai",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "test": "jest --no-coverage",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:integration:watch": "jest --config jest.integration.config.js --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:bdd": "start-server-and-test 'pnpm start' http://localhost:3000 'cucumber-js'",
    "test:bdd:only": "cucumber-js",
    "test:bdd:dev": "cucumber-js --profile dev",
    "test:bdd:ci": "cucumber-js --profile ci",
    "test:bdd:watch": "nodemon --watch features --exec 'cucumber-js --profile dev'",
    "test:all": "npm run test && npm run test:bdd && npm run test:e2e",
    "test:perf": "jest --testPathPattern=performance --no-coverage",
    "test:load": "tsx src/__tests__/performance/load-test.ts",
    "test:acceptance": "playwright test e2e/acceptance/",
    "test:acceptance:ui": "playwright test e2e/acceptance/ --ui",
    "test:acceptance:headed": "playwright test e2e/acceptance/ --headed",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@auth/prisma-adapter": "^2.11.1",
    "@browserbasehq/stagehand": "^3.0.6",
    "@hcaptcha/react-hcaptcha": "^2.0.2",
    "@hello-pangea/dnd": "^18.0.1",
    "@sentry/nextjs": "^10.39.0",
    "@testing-library/dom": "^10.4.1",
    "@vercel/analytics": "^1.6.1",
    "date-fns": "^4.1.0",
    "firebase": "^12.9.0",
    "firebase-admin": "^13.6.1",
    "lucide-react": "^0.562.0",
    "next": "16.1.6",
    "next-auth": "^5.0.0-beta.30",
    "openai": "^4.73.0",
    "playwright": "^1.57.0",
    "prettier": "^3.2.5",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "resend": "^6.9.2",
    "stripe": "^20.3.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@cucumber/cucumber": "^12.4.0",
    "@playwright/test": "^1.57.0",
    "@prisma/adapter-libsql": "^7.4.0",
    "@swc/core": "^1.15.11",
    "@swc/jest": "^0.2.39",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autocannon": "^8.0.0",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.5.0",
    "dotenv": "^16.4.5",
    "eslint": "^4.1.1",
    "eslint-config-next": "^0.2.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "start-server-and-test": "^2.1.3",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,md,yml,yaml,css,scss}": [
      "prettier --write"
    ]
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
